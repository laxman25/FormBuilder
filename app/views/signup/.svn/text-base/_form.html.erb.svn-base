# and open the template in the editor.
%>

<table>
        <tr>
                <td><%= form.label :first_name %></td>
                <td><%= form.text_field :first_name ,:placeholder=>" first name" %></td>
        </tr>
        <tr>
                <td><%= form.label :last_name %></td>
                <td><%= form.text_field :last_name ,:placeholder=>"lasr name" %></td>
        </tr>
        <tr>
                <td><%= form.label :email %></td>
                <td><%= form.text_field :email ,:placeholder=>"email" %></td>
        </tr>
        <tr>
                <td><%= form.label :phone_number %></td>
                <td><%= form.text_field :phone_number ,:placeholder=>"phone number" %></td>
        </tr>
        <tr>
                <td><%= form.label :password %></td>
                <td><%= form.password_field :password ,:placeholder=>"password" %></td>
        </tr>
        <tr>
                <td><%= form.label :password_confirmation %></td>
                <td><%= form.password_field :password_confirmation ,:placeholder=>"confirm password"%></td>
        </tr>
</table>
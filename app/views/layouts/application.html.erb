<!DOCTYPE html>
<html>
	<head>
		<title>Trunk</title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag  :defaults,"application.js","rails.validations", "rails.validations.custom","http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js","create.rjs","rails.validations.custom.js","jquery-1.7.1.js","tabs.js","categories.js.coffee","tabs.js","rails.js"%>
		<%#= javascript_include_tag :defaults,"application","rails.validations", "rails.validations.custom" %>
		<%= csrf_meta_tags %>
	</head>
	<body>
		<div id="wrapper">
			<div id="header">
				<h1>Dynamic Form Builder</h1>
			</div>
			<div id="sessions" >
				<div class="left">
					Welcome <%= current_user.first_name %> <%= current_user.last_name %> !
				</div>
				<div class="right">
					<%= button_to '' , logout_path, :method => :delete , :class=>"logout" %>
				</div>
			</div>
			<div id="navigator">
				
					<% if current_user.is_admin? || current_user.role.name  == "Administrator" %>
					<ul><li>
						<a href=<%= users_path  %>>Users</a>
					</li></ul>
					<% end %>

					<% if current_user.role_id != 1  %>
					<ul><li>
						<a href=<%= tests_path  %>>Tests</a>
					</li>
					<li>
						<a href=<%= edit_user_path  %>>My Profile</a>
					</li>
                                       
					</ul>
					
					<% end %>
				
			</div>
			<div id="main">
				<%= yield %>
			</div>
			<div class="spacerdiv"></div>
		</div>
	</body>
</html>

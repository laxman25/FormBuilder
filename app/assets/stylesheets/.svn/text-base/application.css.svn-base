/*
 * This is a manifest file that'll automatically include all the stylesheets available in this directory
 * and any sub-directories. You're free to add application-wide styles to this file and they'll appear at
 * the top of the compiled file, but it's generally better to create a new file per style scope.
 *= require_self
 *= require_tree .
 */
input[type=button] {
	border: none;
	border-radius: 5px;
}
#sessions {
	height: 22px;
	width: 1020px;
	padding: 7px 15px;
	background: grey;
	/*         border: 1px solid #000000;*/
}
#sessions .left {
	float: left;
	color: white;
}
#sessions .right {
	float: right;
}
.logout {
	border: none;
	background-image: url("/images/Logout_Btn.png");
	width: 71px;
	height: 25px;
	border-radius: 5px;
}
/*main div*/
.add {
	background: url("/images/Add_Gray_48x48.png") no-repeat;
	background-size: 20% 110%;
	padding-left: 20px;
}
.add:hover {
	background: url("/images/Add_48x48.png") no-repeat;
	background-size: 20% 110%;
	padding-left: 20px;
}
.del {

	background: url("/images/Delete_Gray48x48.png") no-repeat;
	background-size: 25% 110%;
	padding-left: 20px;
}
.del:hover {
	background: url("/images/Delete_48x48.png") no-repeat;
	background-size: 25% 110%;
	padding-left: 20px;
}
#main {
	border: 2px solid #808080;
	border-bottom-right-radius: 5px;
	float: left;
	margin-bottom: -2px;
	padding: 60px 50px;
	width: 810px;
	border-right: 0px;
}
#navigator {
	float: left;
	border-top: 2px solid #808080;
	clear: left;
	height: 38px;
	padding: 40px 15px;
	width: 106px;
	/*        border: 1px solid #000000;*/
}
#navigator ul {
	margin: 0;
	padding: 0;
	list-style-type: none;
	list-style: none;
}
#navigator ul li {

	height: 25px;
	margin-bottom: 15px;
	text-align: center;
	width: 111px;
	/*	display: inline;*/

}
#navigator ul li a, .linksForProfile a {
	text-decoration: none;
	padding: 3px 10px;
	color: white;
	display: block;
	background-image: url('/images/link_bkgd.png');
	background-size: 100% 100%;
}
#navigator ul li a:hover, .linksForProfile a:hover {
	color: #fff;
	background-image: url('/images/link_hover.png');
	background-size: 100% 100%;
}
.spacerdiv {
	float: none;
	margin: 0;
	padding: 0;
	height: 1px;
	font-size: 0;
	line-height: 0;
	clear: both;
}
h1 {
	font-family: Helvetica, sans-serif;
	font-style: oblique;
	font-variant: small-caps;
	font-weight: 400;
	color: #0000E0;
}
table {	
	border-collapse: collapse;
}

.table1 {
	font-family: "Trebuchet MS", sans-serif;
	font-size: 14 px;
	/*    font-weight: bold;*/
	/*    line-height: 1.4em;*/
	font-style: normal;
	border-collapse: collapse;
}
.table1 thead th {
	padding: 10px;	
	text-shadow: 1px 1px 1px #568F23;	
	background: -webkit-gradient(
	linear,
	left bottom,
	left top,
	color-stop(0.02, rgb(123,192,67)),
	color-stop(0.51, rgb(139,198,66)),
	color-stop(0.87, rgb(158,217,41))
	);
	background: -moz-linear-gradient(
	center bottom,
	rgb(123,192,67) 2%,
	rgb(139,198,66) 51%,
	rgb(158,217,41) 87%
	);
		
}
.table1 thead th:empty {
	background: transparent;
	border: none;
}
#prev_created_ques_display {

	height: 100px;
	overflow: auto;
	border: 1px solid #666;
	background-color: #ccc;
	padding: 1px;
}
.field_with_errors {
  display: inline;
}



.field_with_errors .message {
  color: #D00;
  padding-left: 5px;
  font-size: 15px;
}

#table_questions tr td:nth-child(1) { width:15px}
#table_questions tr td:nth-child(2) { width:550px}
#table_questions tr td:nth-child(3) { width:135px}

